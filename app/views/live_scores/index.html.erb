<p id="notice"><%= notice %></p>

<h1>Listing Live Scores</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @live_scores.each do |live_score| %>
      <tr>
        <td><%= link_to 'Show', live_score %></td>
        <td><%= link_to 'Edit', edit_live_score_path(live_score) %></td>
        <td><%= link_to 'Destroy', live_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Live score', new_live_score_path %>
